{
  "name": "vip-music",
  "version": "0.1",
  "description": "Discord music bot.",
  "main": "index.ts",
  "author": "Ehsan Ghaffar <ghafari.5000@gmail.com>",
  "private": true,
  "homepage": "https://github.com/ehsanghaffar/vip-music",
  "repository": "github:ehsanghaffar/vip-music",
  "bugs": "https://github.com/ehsanghaffar/vip-music/issues",
  "engines": {
    "node": ">=16.11.0"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "node",
    "typescript",
    "vip"
